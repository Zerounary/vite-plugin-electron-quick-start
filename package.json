{
  "name": "CloudHappy-POS",
  "version": "0.0.5",
  "description": "CloudHappy-POS",
  "main": "dist/electron-main/index.js",
  "repository": "https://github.com/caoxiemeihao/vite-plugin-electron-quick-start.git",
  "author": "CloudHappy <song.c@sungeon.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "commit": "git-cz -a",
    "release": "standard-version -a",
    "build": "npm-run-all build-electron rebuild-sqlite",
    "build-electron": "rimraf dist && vite build && electron-builder",
    "rebuild-sqlite": "cd ./node_modules/better-sqlite3 && node-gyp.cmd clean configure build --verbase --arch=ia32 && cd ../../ && electron-rebuild"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "better-sqlite3": "^7.5.3",
    "echarts": "^5.3.2",
    "electron-log": "^4.4.7",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.0.1",
    "element-plus": "^2.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@iconify/vue": "^3.2.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@windicss/plugin-heropatterns": "^0.0.8",
    "c8": "^7.11.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^18.0.1",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-electron": "^0.4.2",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-windicss": "^1.8.4",
    "vitest": "^0.10.4",
    "windicss": "^3.5.1"
  },
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "quick-start"
  ]
}
